<h1>Editing character_race</h1>

<% form_for(@character_race) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :character_id %><br />
    <%= f.text_field :character_id %>
  </p>
  <p>
    <%= f.label :race_id %><br />
    <%= f.text_field :race_id %>
  </p>
  <p>
    <%= f.label :parts %><br />
    <%= f.text_field :parts %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @character_race %> |
<%= link_to 'Back', character_races_path %>
