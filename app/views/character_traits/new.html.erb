<h1>New character_trait</h1>

<% form_for(@character_trait) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :character_id %><br />
    <%= f.text_field :character_id %>
  </p>
  <p>
    <%= f.label :trait_id %><br />
    <%= f.text_field :trait_id %>
  </p>
  <p>
    <%= f.label :race_id %><br />
    <%= f.text_field :race_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', character_traits_path %>
